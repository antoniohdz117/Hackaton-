{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,          
    "checkJs": false,         
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],         
  "exclude": ["node_modules"]
}
